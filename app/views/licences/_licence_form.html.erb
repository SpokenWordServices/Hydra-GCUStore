<%=form_for @licence do |f|%>
  <%= f.label :name  %>
  <%= f.text_field :name%>
  <%= f.label :description  %>
  <%= f.text_area  :description, :size =>'60x12'%>
  <%= f.label :link  %>
  <%= f.url_field :link%>
  <%= f.label "Allow Downloads" %>
  <%= f.check_box :download %>	
  <%= f.submit 'Save'%>
<%end%>
